# Inngest Configuration
INNGEST_SIGNING_KEY=signkey_prod_xxxxx...
INNGEST_EVENT_KEY=your-app-id

# Existing variables
NEXT_PUBLIC_SUPABASE_URL=https://xxx.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJ...
SUPABASE_SERVICE_ROLE_KEY=eyJ...
NEXT_PUBLIC_SITE_URL=https://your-domain.com
VIRUSTOTAL_API_KEY=your-virustotal-key
NEXT_PUBLIC_SENTRY_DSN=https://xxx@xxx.ingest.sentry.io/xxx
RESEND_API_KEY=re_xxx
FEEDBACK_EMAIL=feedback@example.com

# BaselineReqs Integration (optional — omit to disable)
# Both apps must share the same INTEGRATION_SECRET value.
# BASELINEREQS_DOMAIN is just the domain (e.g. baselinereqs.com) — the
# subdomain is prepended per-tenant at call time (acme.baselinereqs.com).
BASELINEREQS_DOMAIN=baselinereqs.com
INTEGRATION_SECRET=a-long-random-secret-shared-with-baselinereqs
